{
  "id": "poetry-asdf",
  "version": "1.0.1",
  "name": "Poetry (via asdf)",
  "documentationURL": "http://github.com/aps831/devcontainers-features/tree/master/src/poetry-asdf",
  "description": "",
  "options": {
    "version": {
      "default": "latest",
      "description": "Select the version of Poetry to install.",
      "proposals": ["latest"],
      "type": "string"
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers-contrib/features/asdf-package",
    "ghcr.io/aps831/devcontainers-features/python-asdf"
  ],
  "dependencies": [
    {
      "feature": "ghcr.io/aps831/devcontainers-features/python-asdf",
      "options": {
        "version": "latest"
      }
    },
    {
      "feature": "ghcr.io/devcontainers-contrib/features/asdf-package",
      "options": {
        "plugin": "poetry",
        "version": "$options.version"
      }
    }
  ],
  "install_command": "echo 'Done!'",
  "test_command": "poetry --version"
}
